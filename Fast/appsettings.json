{
  //日志
  "Logging": {
    //调试时显示警告（Warning）以上的日志
    "Debug": {
      "LogLevel": { //表示日志的严重性包含（Trace= 0，Debug= 1，Information= 2，Warning= 3，Error= 4，Critical= 5，None= 6
        "Default": "Warning"
      },
      "IncludeScopes": false
    },
    //控制台里在调试时显示警告以上日志
    "Console": { //控制控制台不显示多余的日志
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.Hosting.Lifetime": "Debug"
      },
      "IncludeScopes": false
    },
    "Log4Net": {
      "Name": "FastCore",
      "IncludeScopes": false
    }
  },

  //允许访问的主机（如果设置成 127.0.0.1 就不能在 localhost 打开网站了）
  "AllowedHosts": "*",

  //自定义配置信息
  "ConfigInfo": {
    "CustomFolder": "CustomConfig", //自定义的配置文件文件夹
    "IgnoreConfigurationFiles": [] //要忽略的自定义配置文件
  }
}